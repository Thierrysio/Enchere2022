<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Enchere2022.Vues.InscriptionVue">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Image x:Name="image" />
            <Button x:Name="toto" Clicked="toto_ClickedAsync"/>
            <Button x:Name="getImage" Text="recuperer image du user" Clicked="getImage_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>